---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbar";
import Footer from "../components/Footer";
import Spacing from "../components/Spacing";
import BlogSection from "../components/BlogSection";
import Luxestudios from "../components/project-page/Luxestudios.jsx";
import "../components/index.css";
import Image from "astro/components/Image.astro";
// image 
import luxestudiosImage from "../assets/luxestudiosweb.jpg"

import {blogs} from "../pages/blog/[slug].astro"
---
<Layout>
    <Navbar showNavManu={false} />
    <Luxestudios><Image src={luxestudiosImage} alt="Sitegardian Image 1" class="mx-auto"/></Luxestudios>
    <BlogSection blogs={blogs}/>    
    <Footer client:load/>
    <Spacing/>
</Layout>